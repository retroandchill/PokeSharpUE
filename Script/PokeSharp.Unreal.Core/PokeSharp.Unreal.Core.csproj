<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <Import Project="..\..\..\UnrealSharp\UnrealSharp.Shared.props" />
    <ItemGroup Condition="'$(PokeSharpSlnDir)' != ''">
        <ProjectReference Include="$(PokeSharpCoreProj)" />
    </ItemGroup>
    <ItemGroup>
        <Reference Include="UnrealSharp.Binds">
            <HintPath>$(UnrealSharpPluginDir)Binaries\Managed\$(TargetFramework)\UnrealSharp.Binds.dll</HintPath>
        </Reference>
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Injectio" />
        <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" />
        <PackageReference Include="Microsoft.Extensions.Logging" />
        <PackageReference Include="Riok.Mapperly" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\PokeSharp.Glue\PokeSharp.Glue.csproj" />
    </ItemGroup>
</Project>
